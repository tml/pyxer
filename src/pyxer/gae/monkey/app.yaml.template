application: __APP_NAME__
version: 1
runtime: python
api_version: 1

handlers:
- url: .*
  script: __APP_SCRIPT__

skip_files: |
 ^(.*/)?(
 (.*\.exe)|
 (.*\.bat)|
 (.*\.pyd)|
 (app\.yaml)|
 (app\.yml)|
 (index\.yaml)|
 (index\.yml)|
 (#.*#)|
 (.*~)|
 (.*\.py[co])|
 (.*/RCS/.*)|
 (Scripts/.*)|
 (bin/.*)|
 ([Ll]ib/encodings/.*)|
 (Lib/[^/]+\.py)|
 ([Ll]ib/.+?/[Ww]eb[Oo]b-.+?\.egg/.*)|
 (cache/.*)|
 (tests/.*)|
 (include/.*)|
 (\..*)|
 )$
